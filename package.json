    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "install": "lerna bootstrap",
    "bootstrap": "lerna bootstrap",
    "start": "concurrently \"npm:start:client\" \"npm:start:server\" ",
    "build": "concurrently \"npm:build:client\" \"npm:build:server\" ",
    "start:client": "ts-node scripts/client/start.ts",
    "start:server": "ts-node scripts/server/start.ts",
    "build:client": "ts-node scripts/client/build.ts",
    "build:server": "ts-node scripts/server/build.ts",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:watchAll": "jest --config jest.config.json --watchAll",
    "test:coverage": "jest --config jest.config.json --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,html,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
