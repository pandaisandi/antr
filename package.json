{
  "name": "antr",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/plugin-transform-regenerator": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/dotenv-webpack": "1.7.0",
    "@types/friendly-errors-webpack-plugin": "0.1.2",
    "@types/html-webpack-plugin": "3.2.1",
    "@types/jest": "24.0.23",
    "@types/jsonwebtoken": "8.3.5",
    "@types/node": "12.12.12",
    "@types/supertest": "2.0.8",
    "@types/terser-webpack-plugin": "2.2.0",
    "@types/webpack": "4.41.0",
    "@types/webpack-dev-middleware": "2.0.3",
    "@types/webpack-dev-server": "3.4.0",
    "@types/webpack-node-externals": "1.7.0",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-typescript-metadata": "0.2.2",
    "concurrently": "5.0.0",
    "css-loader": "3.2.0",
    "dotenv": "8.2.0",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.7.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "4.3.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "9.4.3",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "1.19.1",
    "rxjs": "6.5.3",
    "sql.js": "1.0.1",
    "style-loader": "1.0.0",
    "stylelint": "12.0.0",
    "stylelint-config-css-modules": "2.1.0",
    "stylelint-config-recommended": "3.0.0",
    "supertest": "4.0.2",
    "terser-webpack-plugin": "2.2.1",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.2",
    "tslib": "1.10.0",
    "typescript": "3.7.2",
    "url-loader": "2.3.0",
    "webpack": "4.41.2",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.9.0",
    "webpack-node-externals": "1.7.2"
  },
  "scripts": {
    "start": "concurrently \"npm:start:client\" \"npm:start:server\" ",
    "build": "concurrently \"npm:build:client\" \"npm:build:server\" ",
    "start:client": "ts-node client/scripts/start.ts",
    "start:server": "ts-node server/scripts/start.ts",
    "build:client": "ts-node client/scripts/build.ts",
    "build:server": "ts-node server/scripts/build.ts",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:watchAll": "jest --config jest.config.json --watchAll",
    "test:coverage": "jest --config jest.config.json --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,html,css,json,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
